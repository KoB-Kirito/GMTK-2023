config_version=5

[application]

config/name="GMTK2023"
run/main_scene="res://menu/menu.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

Dialogic="*res://addons/dialogic/Other/DialogicGameHandler.gd"

[dialogic]

animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.4
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
settings/text_speed=0.01
text/skippable=true
text/autoadvance_delay=1.0
text/hide_empty_textbox=true
text/autopauses={}
layout/mode=1
layout/layout_scene="res://gameplay/dialog/style/DialogicLayout.tscn"
animations/join_default="res://addons/dialogic/Modules/Character/DefaultAnimations/fade_in_up.gd"
animations/leave_default="res://addons/dialogic/Modules/Character/DefaultAnimations/fade_out_down.gd"

[display]

window/size/viewport_width=480
window/size/viewport_height=270
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

[filesystem]

import/blender/enabled=false

